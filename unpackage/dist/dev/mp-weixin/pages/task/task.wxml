<view class="container"><view class="select_container"><view class="select_item"><picker mode="selector" value="{{countryIndex}}" range="{{countryList}}" range-key="DCName" data-event-opts="{{[['change',[['countryChanged',['$event']]]]]}}" bindchange="__e"><text>{{countryList[countryIndex].DCName}}</text><text class="iconfont iconjiantou"></text></picker></view><text class="split_text">|</text><view class="select_item"><picker mode="selector" value="{{typeIndex}}" range="{{types}}" range-key="name" data-event-opts="{{[['change',[['typeChanged',['$event']]]],['typeHasChanged',[['typeHasChanged',['$event']]]]]}}" bindchange="__e" bindtypeHasChanged="__e"><text>{{types[typeIndex].name}}</text><text class="iconfont iconjiantou"></text></picker></view></view><scroll-view class="uni-list" scroll-y="true"><block wx:for="{{taskList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="FacilitiesTypeCode"><view class="uni-list-cell"><image class="uni-media-list-logo" src="{{item.PanoramicPhoto}}" mode="scaleToFill"></image><view class="uni-flex uni-column"><text class="uni-title">{{item.FacilitiesName}}</text><text class="uni-text-gray">{{item.FacilitiesAddr}}</text></view></view></block></scroll-view></view>