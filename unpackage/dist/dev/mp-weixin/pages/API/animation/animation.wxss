
.animation-element-wrapper {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	width: 100%;
	padding-top: 150rpx;
	padding-bottom: 150rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	overflow: hidden;
	background-color: #ffffff;
}
.animation-element {
	width: 200rpx;
	height: 200rpx;
	background-color: #1AAD19;
}
.animation-buttons {
	padding:30rpx 0;
	width: 100%;
	height: 360rpx;
}
.animation-button {
	float: left;
	width: 44%;
	margin: 15rpx 3%;
}
.animation-button-reset {
	width: 94%;
}

