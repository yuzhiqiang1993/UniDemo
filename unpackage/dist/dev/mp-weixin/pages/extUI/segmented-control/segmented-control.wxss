.uni-list-item__container:after {
	position: absolute;
	z-index: 3;
	right: 0;
	bottom: 0;
	left: 30rpx;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(.5);
	-ms-transform: scaleY(.5);
	    transform: scaleY(.5);
	background-color: #c8c7cc;
}

