<view class="calendar-content"><text class="calendar-title">日历组件</text><view class="calendar-tags-group"><block wx:for="{{tags}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toggle',[index,'$0'],[[['tags','',index]]]]]]]}}" class="{{['calendar-tags',(item.checked)?'checked':'']}}" bindtap="__e"><view class="calendar-tags-item">{{item.name}}</view></view></block></view><button class="calendar-button" type="button" data-event-opts="{{[['tap',[['open',['$event']]]]]}}" bindtap="__e">打开日历</button><block wx:if="{{timeData.lunar}}"><text class="calendar-title">已选日期</text></block><block wx:if="{{timeData.lunar}}"><view class="calendar-info"><view>{{"当前选择时间 : "+timeData.fulldate}}</view><block wx:if="{{tags[0].checked}}"><view>{{"农历日期 : "+(timeData.year+'年'+timeData.month+'月'+timeData.date+'日 （'+timeData.lunar.astro+')')}}</view></block><block wx:if="{{tags[0].checked}}"><view>{{''+(timeData.lunar.gzYear+'年'+timeData.lunar.gzMonth+'月'+timeData.lunar.gzDay+'日 ('+timeData.lunar.Animal+'年)')+"\n\t\t\t"+(timeData.lunar.IMonthCn+timeData.lunar.IDayCn)+" "+(timeData.lunar.isTerm?timeData.lunar.Term:'')+''}}</view></block></view></block><block wx:if="{{show}}"><view data-event-opts="{{[['tap',[['closeMask',['$event']]]]]}}" class="calendar-mask" bindtap="__e"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="calendar-box" catchtap="__e"><uni-calendar vue-id="1" lunar="{{tags[0].checked}}" fixed-heihgt="{{tags[1].checked}}" slide="{{slide}}" disable-before="{{tags[6].checked}}" start-date="{{startDate}}" end-date="{{endDate}}" date="{{date}}" data-event-opts="{{[['^change',[['change']]],['^toClick',[['toClick']]]]}}" bind:change="__e" bind:toClick="__e" bind:__l="__l"></uni-calendar><view class="calendar-button-groups"><button data-event-opts="{{[['tap',[['closeMask',['$event']]]]]}}" class="calendar-button-confirm" bindtap="__e">取消</button><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="calendar-button-confirm" bindtap="__e">确认</button></view></view></view></block></view>