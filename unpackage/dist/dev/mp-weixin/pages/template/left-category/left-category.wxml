<view class="container"><view class="page-body"><scroll-view class="nav-left" style="{{('height:'+height+'px')}}" scroll-y><block wx:for="{{categoryList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['categoryClickMain',['$0',index],[[['categoryList','',index]]]]]]]}}" class="{{['nav-left-item',index==categoryActive?'active':'']}}" bindtap="__e">{{''+item.NAME+''}}</view></block></scroll-view><scroll-view class="nav-right" style="{{('height:'+height+'px')}}" scroll-y scroll-top="{{scrollTop}}" scroll-with-animation data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><block wx:for="{{subCategoryList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="nav-right-item" id="{{index===0?'first':''}}"><image src="{{item.LOGO}}"></image><view>{{item.NAME}}</view></view></block><block wx:if="{{subCategoryList.length>1}}"><page-foot vue-id="1" name="{{name}}" bind:__l="__l"></page-foot></block></scroll-view></view></view>