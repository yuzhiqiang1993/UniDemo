<view class="uni-indexed"><scroll-view class="uni-indexed__list" style="{{'height:'+(winHeight+'px')+';'}}" scroll-into-view="{{scrollViewId}}" scroll-y><block wx:for="{{lists}}" wx:for-item="list" wx:for-index="idx" wx:key="idx"><block><block wx:if="{{list.items&&list.items.length>0}}"><view class="uni-indexed__list-title" id="{{'uni-indexed-list-'+list.key}}">{{list.key}}</view></block><block wx:if="{{list.items&&list.items.length>0}}"><view class="uni-list"><block wx:for="{{list.items}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-list-item" hover-class="uni-list-item--hover"><view data-event-opts="{{[['tap',[['onClick',[idx,index]]]]]}}" class="uni-list-item__container" bindtap="__e"><block wx:if="{{showSelect}}"><view style="margin-right:20rpx;"><uni-icon vue-id="{{'1-'+idx+'-'+index}}" type="{{item.checked?'checkbox-filled':'circle'}}" color="{{item.checked?'#007aff':'#aaa'}}" size="24" bind:__l="__l"></uni-icon></view></block><view class="uni-list-item__content">{{item.name}}</view></view></view></block></view></block></block></block></scroll-view><view data-event-opts="{{[['touchstart',[['touchStart',['$event']]]],['touchmove',[['touchMove',['$event']]]],['touchend',[['touchEnd',['$event']]]]]}}" class="{{['uni-indexed__menu',touchmove?'active':'']}}" style="{{'height:'+(winHeight+'px')+';'}}" bindtouchstart="__e" catchtouchmove="__e" bindtouchend="__e"><block wx:for="{{lists}}" wx:for-item="list" wx:for-index="key" wx:key="key"><text class="{{['uni-indexed__menu-item',touchmoveIndex==key?'active':'']}}" style="{{'height:'+(itemHeight+'px')+';'+('line-height:'+(itemHeight+'px')+';')}}">{{''+list.key+''}}</text></block></view><block wx:if="{{touchmove}}"><view class="uni-indexed--alert">{{lists[touchmoveIndex].key}}</view></block></view>